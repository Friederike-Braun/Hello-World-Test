pipeline{
  agent any
  tools {
          maven "Maven 3.6.3"
          jdk "JDK-17"
  }

  stages{
    stage('Initialize'){
        steps{
            echo "PATH = ${M2_HOME}/bin:${PATH}"
            echo "M2_HOME = /opt/maven"
        }
    }
    stage('Hello'){
      steps{
        echo 'World'
      }
    }
  }
}


